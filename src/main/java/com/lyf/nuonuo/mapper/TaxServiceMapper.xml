<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyf.nuonuo.dao.TaxServiceDao">
    <insert id="insertTaxService" parameterType="java.util.List">
        insert into portal_buyer_bind_service (c_texnum,c_serviceid)
        values
        <foreach collection="list" item="item" index="index" separator="," >
            (#{item.taxId},#{item.serviceId})
        </foreach>
    </insert>

</mapper>















